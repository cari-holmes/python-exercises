    "# 1. Use matplotlib to plot the following equation: y = x**2 − x + 2\n",
    "# Add an anotation for the point 0, 0, the origin\n",
    "\n",
    "x = list(range(-10, 11))\n",
    "y = [n**2-n+2 for n in x]\n",
    "plt.plot(x,y, c='navy')\n",
    "plt.title('A Quadratic Distribution')\n",
    "plt.xlabel('$x$')\n",
    "plt.ylabel('$y$')\n",
    "plt.annotate('Origin', xy = (0,0),xytext=(-3,5), arrowprops={'facecolor': 'lightblue'})\n",
    "plt.show()"
